{
  "manifest_version": 3,
  "name": "Minimize privacy protection",
  "description": "Hide tab's title when minimizing window",
  "version": "0.1.0",
  "author": "dragonish",
  "homepage_url": "https://github.com/dragonish/minimize-privacy-protection",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "index.js"
  },
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}
